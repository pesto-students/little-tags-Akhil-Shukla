 <span><AiOutlineSearch /></span>\n                    <input type=\"text\" placeholder=\"Search...\"></input>\n                </div>\n                {showLogo ? <div className=\"user-logo\">\n                    <AiOutlineUser />\n                    <span className=\"user-logout-options\">\n\n                        <div className=\"display-name\">Hello, {displayName}</div>\n\n                        <hr></hr>\n                        <div className=\"order-history\">\n                            \n\n                            <div className=\"order-history-content\"><Link>Orders</Link></div>\n                            <div className=\"order-history-content\"> <Link>Wish list</Link></div>\n                            <div className=\"order-history-content\"> <Link>Contact us</Link></div>\n                            <div className=\"order-history-content\"> <Link>Insider</Link></div>\n                            \n                            \n                           \n                        </div>\n                        <hr></hr>\n\n                        <div className=\"log-out-button\" > <button onClick={handleSignOut}>Log Out</button></div>\n                        \n\n                    </span>\n                </div> : <div className=\"user-signin\"><Link to=\"\" onClick={manageSigInModal}><span>Log In/Sign Up</span></Link></div>}\n                <div className=\"cart-logo\"><Link to=\"/cart\"><AiOutlineShoppingCart color='black' />{itemInCart.length > 0  ? <span className=\"store-count\">{itemInCart.length}</span> : null}</Link></div>\n                <div className=\"glob-logo\"><AiOutlineGlobal /></div>\n            </div>\n        </header>\n    )\n}\n\nexport default Header\n","/Users/akhil.shukla/Documents/popes/little-tags-Akhil-Shukla/src/components/ProductDescription/ProductDescription.jsx",[],"/Users/akhil.shukla/Documents/popes/little-tags-Akhil-Shukla/src/components/ProductDescription/ProductDescCard.jsx",["170"],"import React, { useState } from 'react';\nimport { BiDollar, BiShoppingBag } from \"react-icons/bi\";\nimport { AiFillStar } from 'react-icons/ai';\nimport { BsHeartFill } from 'react-icons/bs';\nimport {Link} from 'react-router-dom';\nimport { IoIosStarOutline } from \"react-icons/io\";\nimport './styles.scss';\nimport store from '../../store/index';\nimport { addToWishList } from '../../Actions/WishlistActions';\nimport {addToCart} from '../../Actions/CartActions';\n\nfunction ProductDescCard(props) {\n    const { id, title, price, image, description, category, showSize } = props;\n    const [size, setSize] = useState('M');\n    const [quantity, setQuantity] = useState(0);\n    const options = ['SM', 'M', 'L', 'XL']\n\n\n    const onInputChange = event => setQuantity(event.target.value)\n\n\n    const handleAddToWishList = () => {\n        const data  = {\n            id, title, price,image,description, category\n        };\n        store.dispatch(addToWishList(data,id));\n\n    }\n\n    const handleAddToCart = () => {\n        const data  = {\n            id, title, price,image,description, category\n        };\n        store.dispatch(addToCart(data,id));\n\n    }\n    \n    return (\n        <>\n        <div className=\"product-page-header\">\n            <span><Link to=\"/\" style={{fontSize:'20px'}}>HOME</Link>  / <Link to={`/products/${category}`} style={{fontSize:'20px'}}>{category.toUpperCase()} </Link> / <span style={{fontWeight:500,fontSize:'20px'}}>{title}</span></span>\n        </div>\n        <div className=\"product-desc-container\">\n            <div className=\"product-desc-items\">\n                <img src={image} style={{ height: \"400px\", width: \"400px auto\" }} alt='product-img'></img>\n            </div>\n            <div className=\"product-desc-items\">\n                <div className=\"title\">{title}</div>\n                <div className=\"price\">\n                <div className=\"discounted-price\">\n                        <BiDollar  />{price}\n                    </div>\n                    <div className=\"real-price\">\n                        <BiDollar />{(price * 1.2).toFixed(2)} \n                    </div>\n                    <span className=\"discount\">20% Off</span>\n                </div>\n                <div className=\"stars\">\n                    <AiFillStar color='red' /><AiFillStar color='red' /><AiFillStar color='red' /><AiFillStar color='red' /><IoIosStarOutline /> 4 Reviews</div>\n\n                <div className=\"description\">{description}</div>\n                <div className=\"size-quantity\">\n                    <div className=\"size\">\n                        <p>Size</p>\n                        <select name='selectedOption' onChange={setSize} >\n                            {options.map(i => i === size ? (\n                                <option value={i} selected>\n                                    {i}\n                                </option>\n                            ) : (<option value={i}>{i}</option>))}\n                        </select>\n                    </div>\n                    <div className=\"quantity\">\n                        <p>Quantity</p>\n                        <input type=\"number\" value={quantity} onChange={onInputChange}></input>\n                    </div>\n                </div>\n                <div className=\"hr-line\"></div>\n                <div className=\"buttons\">\n                    \n                    <button className=\"cart-button\" onClick={handleAddToCart}><BiShoppingBag /> &nbsp;Add to Cart</button>\n                    <button className=\"wish-button\" onClick={handleAddToWishList}> <BsHeartFill /> &nbsp;WishList</button>\n                </div>\n\n\n            </div>\n        </div>\n        </>\n    )\n}\n\nexport default ProductDescCard","/Users/akhil.shukla/Documents/popes/little-tags-Akhil-Shukla/src/pages/ProductDescriptionPage/ProductDescriptionPage.jsx",[],"/Users/akhil.shukla/Documents/popes/little-tags-Akhil-Shukla/